#include <iostream>

using namespace std;



const int MAX = 10;  // Tamanho máximo da lista



struct ListaEstatica {

    int elementos[MAX];

    int tamanho;  // Número de elementos inseridos na lista



    ListaEstatica() {

        tamanho = 0;  // Inicializa a lista vazia

    }



    // Função para adicionar um elemento à lista

    void adicionar(int valor) {

        if (tamanho < MAX) {

            elementos[tamanho] = valor;

            tamanho++;

        } else {

            cout << "Lista cheia!" << endl;

        }

    }



    // Função para exibir os elementos da lista

    void exibir() {

        cout << "Lista Estática: ";

        for (int i = 0; i < tamanho; i++) {

            cout << elementos[i] << " ";

        }

        cout << endl;

    }

};



int main() {

    ListaEstatica lista;



    lista.adicionar(10);

    lista.adicionar(20);

    lista.adicionar(30);



    lista.exibir();



    return 0;

} 
